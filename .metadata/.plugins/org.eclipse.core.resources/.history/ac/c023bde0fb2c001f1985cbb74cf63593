
public class Operadores {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int primerNumero = 5;
		int segundoNumero;
		segundoNumero = 10;
		int resultado = primerNumero + segundoNumero;
		System.out.println("La suma de los dos numeros es: " + resultado);
		
	}
}
